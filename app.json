{
  "expo": {
    "name": "sflivingwages",
    "slug": "sflw",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": [ "ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "ios": {
      "supportsTablet" : true,
      "icon": "./assets/icon.png"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#d31623"
      }
    },
     "androidStatusBar": {
      "backgroundColor": "#C2185B"
    },
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#d31623"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"]
  }
}